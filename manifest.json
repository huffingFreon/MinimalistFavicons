{
    "name": "Minimalist Favicons",
    "version": "1.0",
    "description": "Analyzes favicon colors and replaces them with a circle of the most prominent one",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content-scripts.js"],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "tabs",
        "storage",
        "http://*/",
        "https://*/"
    ]
}